<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.VBox?>

<VBox prefHeight="400.0" prefWidth="640.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.MainScreenController">
  <children>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="-1.0" prefWidth="-1.0" VBox.vgrow="ALWAYS">
      <children>
            <Button fx:id="newGameBtn" layoutX="283.0" layoutY="44.0" mnemonicParsing="false" onAction="#newGameClick" text="New Game" />
            <Button fx:id="mapEditorBtn" layoutX="282.0" layoutY="116.0" mnemonicParsing="false" onAction="#mapEditorClick" text="Map Editor" />
            <Button fx:id="helpBtn" layoutX="300.0" layoutY="191.0" mnemonicParsing="false" onAction="#helpClick" text="Help" />
            <Button fx:id="exitBtn" cancelButton="true" layoutX="302.0" layoutY="264.0" mnemonicParsing="false" onAction="#exitClick" text="Exit" />
      </children>
    </AnchorPane>
  </children>
</VBox>
